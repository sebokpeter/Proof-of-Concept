@{
    ViewData["Title"] = "Home Page";
}
<body onload="createSocket()">
    <div class="text-center">
        <h1 class="display-4">Welcome to this Proof of Concept</h1>
        <div>
            <button onclick="startClicked()">Start</button>
            <button onclick="resetClicked()">Reset</button>
            <button onclick="sasClicked()">Start Arriving Sequence</button>
            <button onclick="slsClicked()">Start Leaving Sequence</button>
            <button onclick="disconnectClicked()">Disconnect</button>
        </div>
    </div>
    <div id="text" style="text-align:center">
        <textarea id="plcData" cols="50" disabled></textarea>
    </div>

    <script src="~/js/site.js" type="text/javascript"></script>
</body>


